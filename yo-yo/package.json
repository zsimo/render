{
  "name": "base-webpack-config-with-base-build-and-dev-server",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^4.0.1",
    "events": "^3.0.0",
    "html-webpack-plugin": "^2.28.0",
    "now": "^11.4.6",
    "rimraf": "^2.6.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "yo-yo": "^1.4.1"
  },
  "scripts": {
    "prestart": "rimraf ./public/js/ ./public/index.html",
    "start": "webpack-dev-server",
    "prebuild": "rimraf ./public/js/ ./public/index.html",
    "build": "webpack --env.build",
    "deploy": "./node_modules/now/download/dist/now.exe",
    "all": "concurrently \"npm run start\" \"npm run start:server\""
  },
  "bin": {
    "webpack": "./node_modules/.bin/webpack",
    "webpack-dev-server": "node_modules/.bin/webpack-dev-server"
  }
}
