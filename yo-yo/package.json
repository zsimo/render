{
  "name": "base-webpack-config-with-base-build-and-dev-server",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "events": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.4",
    "now": "^11.4.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^7.1.0",
    "socket-io": "^1.0.0",
    "socket.io-client": "^2.1.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "yo-yo": "^1.4.1"
  },
  "scripts": {
    "prestart": "rimraf ./public/js/ ./public/index.html",
    "start": "webpack-dev-server",
    "prebuild": "rimraf ./public/js/ ./public/index.html",
    "build": "webpack --env.build",
    "deploy": "now --public",
    "all": "concurrently \"npm run build\" \"npm run start:server\""
  },
  "bin": {
    "webpack": "./node_modules/.bin/webpack",
    "webpack-dev-server": "node_modules/.bin/webpack-dev-server",
    "now": "node_modules/.bin/now"
  }
}
