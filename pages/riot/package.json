{
  "name": "base-webpack-config-with-base-build-and-dev-server",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "events": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.4",
    "object-assign": "^4.1.1",
    "rimraf": "^2.6.1",
    "riot": "^3.13.2",
    "riot-compiler": "^3.5.3",
    "riot-tag-loader": "^2.1.0",
    "sass-loader": "^7.1.0",
    "socket-io": "^1.0.0",
    "socket.io-client": "^2.1.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.11",
    "webpack-manifest-plugin": "^2.0.4",
    "yo-yo": "^1.4.1"
  },
  "scripts": {
    "rimraf": "rimraf ./public/js/ ./public/index.html ../../public/dist/riot ../../config/webpack_manifest.php",
    "clear": "cd ../../ && php artisan clear-all",
    "prestart": "npm run rimraf && npm run clear",
    "start": "cross-env NODE_ENV=development webpack-dev-server --env.devserver",
    "prebuild": "npm run prestart",
    "build": "cross-env NODE_ENV=development webpack",
    "preprod": "npm run prestart",
    "prod": "cross-env NODE_ENV=production webpack"
  },
  "bin": {
    "webpack": "./node_modules/.bin/webpack",
    "webpack-dev-server": "node_modules/.bin/webpack-dev-server"
  }
}
